{
  "name": "@senswap/sen-scripts",
  "description": "Convenient scripts for keypair creation, contract deployment, and many others",
  "version": "0.0.11",
  "author": "tuphan-dn @tuphan-dn",
  "bin": {
    "sen-scripts": "./bin/run"
  },
  "bugs": "https://github.com/DescartesNetwork/sen-scripts/issues",
  "dependencies": {
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-help": "^3.2.3",
    "@solana/web3.js": "^1.24.1",
    "fs-extra": "^10.0.0",
    "ora": "^5.4.1",
    "tslib": "^1.14.1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.0",
    "@oclif/test": "^1.2.8",
    "@types/chai": "^4.2.21",
    "@types/fs-extra": "8.1.0",
    "@types/mocha": "^5.2.7",
    "@types/node": "^10.17.60",
    "chai": "^4.3.4",
    "globby": "^10.0.2",
    "husky": "^7.0.0",
    "mocha": "^5.2.0",
    "nyc": "^14.1.1",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.10"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/DescartesNetwork/sen-scripts",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "sen-scripts",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "DescartesNetwork/sen-scripts",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "npm run build && oclif-dev manifest && oclif-dev readme",
    "build": "rm -rf lib && tsc -b",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md",
    "prepare": "husky install"
  },
  "types": "lib/index.d.ts"
}
